<map
    id="map"
    longitude="{{longitude}}"
    latitude="{{latitude}}"
    scale="{{scale}}"
    controls="{{controls}}"
    bindcontroltap="controltap"
    markers="{{markers}}"
    bindmarkertap="markertap"
    polyline="{{polyline}}"
    bindregionchange="regionchange"
    show-location
    style="width: 100%; height:{{mapHeight}}px;">

</map>
<view class="foot" style="width: 100%;">
<button type="default"
size="{{defaultSize}}" 
 > 
 点击设置作业区
  </button>
</view>

